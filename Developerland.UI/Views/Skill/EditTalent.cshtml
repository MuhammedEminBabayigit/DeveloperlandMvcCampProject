@model EntityLayer.Concrete.Skill
@{
    ViewBag.Title = "EditTalent";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Yetenek Ekle</h2>


@using (Html.BeginForm("EditTalent", "Skill", FormMethod.Post))
{
    <div class="form-group">
        <div class="col-md-5">
            @Html.HiddenFor(t => t.SkillID)
            @Html.Label("Yetenek Adı")
            @Html.TextBoxFor(t => t.SkillName, new { @class = "form-control" })
        </div>
        <br />
        <div class="col-md-5">
            @Html.Label("Oran")
            @Html.EditorFor(t => t.SkillRange, new { htmlAttributes = new { @class = "form-control" } })
            <br />
        </div>

        <button class=" btn btn-outline-success">Güncelle</button>
    </div>
}
<script src="~/AdminLTE-3.0.4/docs/assets/plugins/jquery/jquery.min.js"></script>
